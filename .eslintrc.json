{
  "env": {
    "browser": false,
    "es2021": true,
    "node": true
  },
  "plugins": [
    "@typescript-eslint"
  ],
  "extends": [
    "./node_modules/ts-standard/eslintrc.json"
  ],
  "overrides": [
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "project": "./tsconfig.json"
  },
  "rules": {
    "no-console": "warn",
    "no-unused-vars": "warn",
    "@typescript-eslint/no-unused-vars": "warn",
    "no-trailing-spaces": "warn",
    "eol-last": "warn",
    "@typescript-eslint/no-misused-promises": "off"
  }
}